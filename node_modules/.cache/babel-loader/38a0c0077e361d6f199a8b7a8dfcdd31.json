EAAA,MAAA,CAAA,oBAAA;;AACA,cAAA,MAAA,CAAA,oBAAA,IAAA,SAAA,EAAA;AACA,gBAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,eAAA,KAAA,IAAA,EAAA;AACA,cAAA,MAAA,CAAA,iCAAA,GACA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,eAAA,CAAA,4BADA;AAEA,cAAA,MAAA,CAAA,0BAAA,GACA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,eAAA,CAAA,iBAAA,IAAA,KADA;AAEA,cAAA,MAAA,CAAA,mBAAA,GAAA,eAAA;AACA,cAAA,MAAA,CAAA,qBAAA,GAAA,IAAA;AACA;AACA,WATA,MASA,IAAA,MAAA,CAAA,oBAAA,IAAA,aAAA,EAAA;AACA,YAAA,MAAA,CAAA,uBAAA,GAAA,eAAA;AACA,YAAA,MAAA,CAAA,yBAAA,GAAA,IAAA;AACA,WAHA,MAGA,IAAA,MAAA,CAAA,oBAAA,IAAA,SAAA,EAAA;AACA,YAAA,MAAA,CAAA,mBAAA,GAAA,eAAA;AACA,YAAA,MAAA,CAAA,qBAAA,GAAA,IAAA;AACA;AACA;AACA,OAvCA,EAwCA,KAxCA,CAwCA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,KAAA;;AACA,YACA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAAA,IACA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GADA,IAEA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAHA,EAIA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,iBAAA,EAAA,CAAA,CAAA,QAAA,CAAA,IAAA;AACA;AACA,OAlDA;AAmDA,KApEA;AAqEA,IAAA,mBArEA,iCAqEA;AAAA;;AACA,MAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,cADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,IAAA,EAAA,qEAHA;AAIA,QAAA,gBAAA,EAAA,IAJA;AAKA,QAAA,kBAAA,EAAA,SALA;AAMA,QAAA,iBAAA,EAAA,MANA;AAOA,QAAA,iBAAA,EAAA;AAPA,OAAA,CAAA,CAQA,IARA,CAQA,UAAA,MAAA,EAAA;AACA,YAAA,MAAA,CAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,IAAA;AACA,UAAA,KAAA,CACA,IADA,CAEA,SAAA,CAAA,gCAAA,EAFA,EAGA;AACA,YAAA,UAAA,EAAA,MAAA,CAAA,SADA;AAEA,YAAA,YAAA,EAAA,MAAA,CAAA,WAFA;AAGA,YAAA,WAAA,EAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,WAHA;AAIA,YAAA,MAAA,EAAA;AAJA,WAHA,EASA;AACA,YAAA,OAAA,EAAA;AACA,cAAA,aAAA,mBAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AADA;AADA,WATA,EAeA,IAfA,CAeA,UAAA,QAAA,EAAA;AACA,YAAA,MAAA,CAAA,WAAA,GAAA,KAAA;;AACA,gBAAA,QAAA,CAAA,IAAA,CAAA,MAAA,KAAA,SAAA,EAAA;AACA,cAAA,MAAA,CAAA,uBAAA;;AACA,cAAA,IAAA,CAAA;AACA,gBAAA,KAAA,EAAA,sBADA;AAEA,gBAAA,IAAA,EAAA,gDAFA;AAGA,gBAAA,gBAAA,EAAA,KAHA;AAIA,gBAAA,IAAA,EAAA;AAJA,eAAA,CAAA;AAMA,aARA,MAQA;AACA,cAAA,MAAA,CAAA,uBAAA;;AACA,cAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA,OADA;AAEA,gBAAA,KAAA,EAAA,uBAFA;AAGA,gBAAA,gBAAA,EAAA,KAHA;AAIA,gBAAA,IAAA;AAJA,eAAA,CAAA;AAOA;AACA,WAnCA,EAoCA,KApCA,CAoCA,UAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,WAAA,GAAA,KAAA;;AACA,gBAAA,OAAA,CAAA,CAAA,CAAA,QAAA,CAAA,EAAA;AACA,kBACA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAAA,IACA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GADA,IAEA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAHA,EAIA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,iBAAA,EAAA,CAAA,CAAA,QAAA,CAAA,IAAA;AACA;;AACA,cAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA,OADA;AAEA,gBAAA,KAAA,EAAA,uBAFA;AAGA,gBAAA,gBAAA,EAAA,KAHA;AAIA,gBAAA,IAAA;AAJA,eAAA,CAAA;AAMA,aAdA,MAcA;AACA,cAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA,OADA;AAEA,gBAAA,KAAA,EAAA,uBAFA;AAGA,gBAAA,gBAAA,EAAA,KAHA;AAIA,gBAAA,IAAA;AAJA,eAAA,CAAA;AAOA;AACA,WA7DA;AA8DA;AACA,OA1EA;AA2EA,KAjJA;AAkJA,IAAA,gBAlJA,8BAkJA;AAAA;;AACA,WAAA,KAAA,CAAA,oBAAA,CAAA,MAAA;AACA,WAAA,KAAA,CAAA,oBAAA,CAAA,iBAAA,CAAA,CAAA,EAAA,KAAA;AACA,MAAA,QAAA,CAAA,WAAA,CAAA,MAAA;AACA,MAAA,CAAA,CAAA,0BAAA,CAAA,CAAA,OAAA,CAAA,MAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,CAAA,CAAA,0BAAA,CAAA,CAAA,OAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,mBAAA;AACA,OAHA,EAGA,IAHA,CAAA;AAIA;AA3JA;AA1DA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"spinner\" id=\"spinner\" v-show=\"big_spinner\"></div>\n    <div\n      v-if=\"\n        (shopify_details_saved &&\n          shopify_sync_status === 'Sync Finished' &&\n          website_name == 'Shopify' &&\n          !is_shopify_public_app_for_company) ||\n          (website_name !== 'Shopify' && website_name !== 'Magento')\n      \"\n    >\n      <h5>Guide:</h5>\n      <ol class=\"pl-3\">\n        <li v-if=\"website_name == 'Woocommerce'\">\n          Your license key is:\n          <i\n            class=\"far fa-copy\"\n            style=\"cursor: pointer\"\n            id=\"copy_licnse_key_tooltip\"\n            data-toggle=\"tooltip\"\n            data-placement=\"top\"\n            title=\"Copied to clipboard!\"\n            @click=\"copy_license_key\"\n          ></i> \n           <textarea\n            class=\"form-control\"\n            ref=\"license_key_textarea\"\n            name=\"license_key\"\n            id=\"license_key\"\n            v-model=\"$session.get('UserInformation').license_key\"\n            readonly\n            style=\"width: 80%; padding: 10px 15px\"\n            rows=\"3\"\n          ></textarea>\n        </li>\n        <li class=\"mb-2\" v-else>\n          Copy the code below\n          <div class=\"form-group mb-2 mr-5\">\n            <textarea\n              class=\"form-control\"\n              v-model=\"widget_code\"\n              rows=\"7\"\n              ref=\"widget_textarea\"\n              readonly\n            ></textarea>\n          </div>\n          <div class=\"btn-wrap text-left mt-0 pt-0\">\n            <button\n              type=\"click\"\n              class=\"darkblue-btn ml-0\"\n              id=\"copy_code_tooltip\"\n              data-toggle=\"tooltip\"\n              data-placement=\"top\"\n              title=\"Copied to clipboard!\"\n              @click=\"copy_widget_code\"\n            >\n              Copy\n            </button>\n          </div>\n        </li>\n        <li>\n          <template v-if=\"website_name == 'Shopify'\">\n            Follow the steps\n            <a href=\"https://cense.ai/documentation/shopify\" target=\"_blank\"\n              >here</a\n            >\n          </template>\n          <template v-if=\"website_name == 'Woocommerce'\">\n            Follow the steps\n            <a href=\"https://cense.ai/documentation/wordpress\" target=\"_blank\"\n              >here</a\n            >\n          </template>\n          <!-- <template v-if=\"website_name == 'Magento'\">\n            Follow the steps\n            <a href=\"\" target=\"_blank\"\n              >here</a\n            >\n          </template> -->\n          <template v-if=\"website_name == 'Squarespace'\">\n            Follow the steps\n            <a href=\"https://cense.ai/documentation/squarespace\" target=\"_blank\"\n              >here</a\n            >\n          </template>\n          <template v-if=\"website_name == 'Weebly'\">\n            Follow the steps\n            <a href=\"https://cense.ai/documentation/weebly\" target=\"_blank\"\n              >here</a\n            >\n          </template>\n          <template v-if=\"website_name == 'Wix'\">\n            Follow the steps\n            <a href=\"https://cense.ai/documentation/wix\" target=\"_blank\"\n              >here</a\n            >\n          </template>\n        </li>\n      </ol>\n    </div>\n    <div\n      v-if=\"\n        shopify_details_saved &&\n        shopify_sync_status === 'Sync Finished' &&\n        website_name == 'Shopify' &&\n        is_shopify_public_app_for_company\n      \"\n      style=\"margin-bottom: 1rem\"\n    >\n\n      <h4>Add Widget to Store Website</h4>\n      <!-- <p>\n          (This will add the cense widget to your online store's website. You can do this anytime later as well.)\n        </p>\n        <button class=\"darkblue-btn ml-0\" @click=\"add_widget_to_store\" v-if=\"!Boolean(is_widget_added_to_website.added)\">\n          Add Widget\n        </button>  \n        <div v-else>\n          <p>\n            Widget Code Added to Your Website! <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\n          </p>\n        </div> -->\n      Follow these steps to activate the Cense Widget on your website:\n      <ol>\n        <li>\n          From your Shopify admin, go to <strong>Online Store</strong> &gt;\n          <strong>Themes</strong>.\n        </li>\n        <li>\n          Find the theme that you want to edit, and then click\n          <strong>Customize</strong>.\n        </li>\n        <li>Click <strong>Theme settings</strong>.</li>\n        <li>Click the <strong>App embeds</strong> tab.</li>\n        <li>\n          You will see the <strong> Cense AI Assistant </strong> embed.\n        </li>\n        <li>\n          Click on the expand icon besides the name of the App to open its settings where\n          you'll have to enter <strong>License Key</strong>.\n        </li>\n        <li>\n          Copy the <strong>License Key</strong> from below and paste it in the\n          text box.\n        </li>\n        <li>\n          To activate the Cense AI Assistant on your store website, click the\n          <strong> toggle </strong> to activate it.\n        </li>\n        <li>\n          You can deactivate the Cense AI Assistant on your store website anytime you wish, you just have to click the\n          <strong> toggle </strong> again to deactivate it.\n        </li>\n      </ol>\n      <div class=\"col-sm-8\">\n        <div style=\"width: auto\">\n          <h5 class=\"mb-sm-0\" style=\"display: inline\">\n            License Key\n            <i\n              class=\"far fa-copy\"\n              style=\"cursor: pointer\"\n              id=\"copy_licnse_key_tooltip\"\n              data-toggle=\"tooltip\"\n              data-placement=\"top\"\n              title=\"Copied to clipboard!\"\n              @click=\"copy_license_key\"\n            ></i>\n          </h5>\n        </div>\n        <textarea\n          class=\"form-control\"\n          ref=\"license_key_textarea\"\n          name=\"license_key\"\n          id=\"license_key\"\n          v-model=\"current_license_key\"\n          readonly\n          style=\"width: 100%; padding: 10px 15px\"\n          rows=\"4\"\n        ></textarea>\n      </div>\n    </div>\n    <div\n      v-if=\"\n        magento_details_saved &&\n        magento_sync_status === 'Sync Finished' &&\n        website_name == 'Magento'\n      \"\n      style=\"margin-bottom: 1rem\"\n    >\n      <h4>Add Cense AI Bot Extension to Store Website</h4>\n      Follow these steps to activate the Cense AI Bot on your website:\n      <ol>\n        <li>\n          Under your root folder, run the following command lines:\n          <ol>\n          <li>composer require censeai/cense-ai-chabot</li>\n          <li>php bin/magento setup:upgrade</li>\n          </ol>\n        </li>\n        <li>\n          Go to the Magento Admin Panel, navigate to the Stores -> Find CENSEAIINC on the right side menu.\n        </li>\n        <li>Go to Settings , Enable Bot (Store View) = Yes , Copy the <strong>License Key</strong> from below and paste it in the\n          text box.</li>\n        <li>Go to the storefront, Check the chatbot in the right hand side bottom corner</li>\n      </ol>\n      <div class=\"col-sm-8\">\n        <div style=\"width: auto\">\n          <h5 class=\"mb-sm-0\" style=\"display: inline\">\n            License Key\n            <i\n              class=\"far fa-copy\"\n              style=\"cursor: pointer\"\n              id=\"copy_licnse_key_tooltip\"\n              data-toggle=\"tooltip\"\n              data-placement=\"top\"\n              title=\"Copied to clipboard!\"\n              @click=\"copy_license_key\"\n            ></i>\n          </h5>\n        </div>\n        <textarea\n          class=\"form-control\"\n          ref=\"license_key_textarea\"\n          name=\"license_key\"\n          id=\"license_key\"\n          v-model=\"current_license_key\"\n          readonly\n          style=\"width: 100%; padding: 10px 15px\"\n          rows=\"4\"\n        ></textarea>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { bus } from \"@/portal/main\";\nimport axios from \"axios\";\nimport api_calls from \"@/portal/api_calls\";\nimport swal from \"sweetalert2\";\nexport default {\n  name: \"ShopifyDetails\",\n  data() {\n    return {\n      shopify_sync_status: \"\",\n      shopify_details_saved: \"\",\n      woocommerce_sync_status: \"\",\n      woocommerce_details_saved: \"\",\n      magento_sync_status: \"\",\n      magento_details_saved: \"\",\n      widget_code: \"\",\n      widget_shopify_code: \"\",\n      widget_general_code: \"\",\n      companyname: this.$session.get(\"UserInformation\").company_name,\n      companyid: this.$session.get(\"UserInformation\").company_id,\n      is_shopify_public_app_for_company: false,\n      big_spinner: false,\n      is_widget_added_to_website: null,\n      retail_web_framework: \"\",\n      current_license_key: this.$session.get(\"UserInformation\").license_key,\n    };\n  },\n  props: {\n    website_name: {\n      type: String,\n      default: \"\",\n    },\n  },\n  mounted() {\n    this.get_integration_details();\n    this.initialize_tooltips();\n    this.widget_general_code =\n      `<link href=\"https://resource.cense.ai/css/app.css\" rel=\"stylesheet\"> <bot-chat></bot-chat> <script src=\"https://resource.cense.ai/js/app.js\"><\\/script> <script>var widget=GenerateWidget({source:'Web',license_key:'${\n        this.$session.get(\"UserInformation\").license_key\n      }'})<\\/script>`.toString();\n    this.widget_shopify_code =\n      `<link href=\"https://resource.cense.ai/css/app.css\" rel=\"stylesheet\"> <bot-chat></bot-chat> <script src=\"https://resource.cense.ai/js/app.js\"><\\/script> <script src=\"https://resource.cense.ai/js/shopify.js\"><\\/script> <script>var widget=GenerateWidget({source:'Web',license_key:'${\n        this.$session.get(\"UserInformation\").license_key\n      }'})<\\/script>`.toString();\n    this.widget_code = this.widget_shopify_code;\n    bus.$on(\"shopify_sync_status\", (data) => {\n      if (this.shopify_sync_status !== \"Sync Finished\") {\n        this.shopify_sync_status = data;\n      }\n    });\n    bus.$on(\"shopify_details_saved\", (data) => {\n      this.shopify_details_saved = data;\n    });\n  },\n  watch: {\n    website_name(newVal) {\n      if (newVal === \"Shopify\") {\n        this.widget_code = this.widget_shopify_code;\n      } else {\n        this.widget_code = this.widget_general_code;\n      }\n    },\n  },\n  methods: {\n    copy_widget_code() {\n      this.$refs.widget_textarea.select();\n      document.execCommand(\"copy\");\n      $(\"#copy_code_tooltip\").tooltip(\"show\");\n      setTimeout(() => {\n        $(\"#copy_code_tooltip\").tooltip(\"dispose\");\n        this.initialize_tooltips();\n      }, 5000);\n    },\n    initialize_tooltips() {\n      $('[data-tooltip=\"tooltip\"]').tooltip({\n        trigger: \"manual\",\n      });\n    },\n    get_integration_details() {\n      this.big_spinner = true;\n      axios\n        .post(\n          api_calls.chatbot_integration_details(),\n          {\n            is_get: true,\n            company_name: this.companyname,\n            company_id: this.companyid,\n            email: this.$session.get(\"UserInformation\").email,\n            license_key: this.$session.get(\"UserInformation\").license_key,\n            token: this.$session.get(\"UserInformation\").tokens,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${this.$session.get(\"at\")}`,\n            },\n          }\n        )\n        .then((response) => {\n          this.big_spinner = false;\n          if (response.data.status === \"success\") {\n            this.retail_web_framework = response.data.data.retail_web_framework;\n            this.$session.set(\"retail_web_framework\", this.retail_web_framework);\n            if (this.retail_web_framework == \"shopify\") {\n              if (response.data.data.shopify_details !== null) {\n                this.is_shopify_public_app_for_company =\n                  response.data.data.shopify_details.shopify_public_app_installed;\n                this.is_widget_added_to_website =\n                  response.data.data.shopify_details.widget_on_website || false;\n                  this.shopify_sync_status = \"Sync Finished\";\n                  this.shopify_details_saved = true;\n              }\n            }else if (this.retail_web_framework == \"woocommerce\"){\n              this.woocommerce_sync_status = \"Sync Finished\";\n              this.woocommerce_details_saved = true;\n            }else if (this.retail_web_framework == \"magento\"){\n              this.magento_sync_status = \"Sync Finished\";\n              this.magento_details_saved = true;\n            }\n          }\n        })\n        .catch((e) => {\n          this.big_spinner = false;\n          if (\n            e.response.status === 410 ||\n            e.response.status === 440 ||\n            e.response.status === 409\n          ) {\n            this.is_integrated = false;\n            this.$root.$emit(\"Session_Expired\", e.response.data);\n          }\n        });\n    },\n    add_widget_to_store() {\n      swal({\n        title: \"Are you sure\",\n        type: \"question\",\n        text: \"This action will add the Cense Chatbot Widget to your Online Store.\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, Add!!\",\n      }).then((result) => {\n        if (result.value) {\n          this.big_spinner = true;\n          axios\n            .post(\n              api_calls.add_widget_code_to_shopify_store(),\n              {\n                company_id: this.companyid,\n                company_name: this.companyname,\n                license_key: this.$session.get(\"UserInformation\").license_key,\n                is_add: true,\n              },\n              {\n                headers: {\n                  Authorization: `Bearer ${this.$session.get(\"at\")}`,\n                },\n              }\n            )\n            .then((response) => {\n              this.big_spinner = false;\n              if (response.data.status === \"Success\") {\n                this.get_integration_details();\n                swal({\n                  title: \"Added Successfully!!\",\n                  text: \"The Widget Code has been added to your store!!\",\n                  showCancelButton: false,\n                  type: \"success\",\n                });\n              } else {\n                this.get_integration_details();\n                swal({\n                  type: \"error\",\n                  title: \"Some Error Occurred!!\",\n                  showCancelButton: false,\n                  html: `<p> We encountered an error while adding the code to your website. Please try again. If this error persits, kindly contact admin \n                at <a href=\"mailto:hello@cense.ai\">hello@cense.ai</a> for further help.</p>`,\n                });\n              }\n            })\n            .catch((e) => {\n              this.big_spinner = false;\n              if (Boolean(e.response)) {\n                if (\n                  e.response.status === 410 ||\n                  e.response.status === 440 ||\n                  e.response.status === 409\n                ) {\n                  this.$root.$emit(\"Session_Expired\", e.response.data);\n                }\n                swal({\n                  type: \"error\",\n                  title: \"Some Error Occurred!!\",\n                  showCancelButton: false,\n                  html: `Please try again.`,\n                });\n              } else {\n                swal({\n                  type: \"error\",\n                  title: \"Some Error Occurred!!\",\n                  showCancelButton: false,\n                  html: `<p> We encountered an error while adding the code to your website. Please try again. If this error persits, kindly contact admin \n                at <a href=\"mailto:hello@cense.ai\">hello@cense.ai</a> for further help.</p>`,\n                });\n              }\n            });\n        }\n      });\n    },\n    copy_license_key() {\n      this.$refs.license_key_textarea.select();\n      this.$refs.license_key_textarea.setSelectionRange(0, 99999);\n      document.execCommand(\"copy\");\n      $(\"#copy_licnse_key_tooltip\").tooltip(\"show\");\n      setTimeout(() => {\n        $(\"#copy_licnse_key_tooltip\").tooltip(\"dispose\");\n        this.initialize_tooltips();\n      }, 5000);\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/portal/Chatbot/Dashboard/Integrations/Components"}]}