      show_footer === true)? 'position: relative; min-height: 100vh; padding-bottom: 3.5rem;': '' \">\n    <div v-if=\"is_maintainence\">\n     <maintenance></maintenance>\n    </div>\n    <div v-else>\n      <scale-transition :duration=\"300\" mode=\"out-in\">\n        <router-view></router-view>\n      </scale-transition>\n      <!-- <bot-chat v-show=\"show_bot\"></bot-chat> -->\n    </div>\n    <footer\n      class=\"footer\"\n      style=\"margin-top\"\n      v-if=\"show_footer_component\"\n    >\n      <!-- harsh -->\n      <p :style=\"change_margin\" class=\"mt-3\">\n        â’¸ {{ new Date().getFullYear() }} Cense AI Inc. All rights reserved.\n        <a href=\"https://cense.ai/privacy-policy\">Privacy Policy</a>\n      </p>\n    </footer>\n  </div>\n</template>\n\n<script>\n// import BotChat from \"@/portal/Chatbot/Dashboard/BotChat\";\nimport { ScaleTransition, ZoomCenterTransition } from \"vue2-transitions\";\nimport Maintenance from \"./Chatbot/Dashboard/Layout/Maintenance.vue\"\nimport axios from 'axios';\nimport api_calls from \"@/portal/api_calls\";\nimport { bus } from \"@/portal/main\";\nimport $ from \"jquery\";\nexport default {\n  components: {\n    // BotChat\n    Maintenance,\n    ScaleTransition,\n    ZoomCenterTransition,\n  },\n  data() {\n    return {\n      show_bot: this.$session.has(\"UserInformation\") ? true : false,\n      show_footer: false,\n      is_maintainence: false,\n    };\n  },\n    computed: {\n    show_footer_component() {\n      if (\n        this.$route.name != \"Direct Response Bot\" &&\n        this.$route.name != \"Login\" &&\n        this.$route.name != \"Register\" &&\n        this.$route.name != \"Unsubscribe\"&&\n        this.show_footer === true\n      ) {\n        return true;\n      }\n      return false;\n    }\n  },\n  beforeCreate(){\n    this.$session.set(\"IsPortalMaintainence\", false)\n    axios.get(api_calls.maintainence_check_endpoint(), {\n      params: {\n        is_get: true\n      }\n    })\n    .then((response) => {\n      if(response.data){\n        this.is_maintainence = true;\n      } else {\n        this.is_maintainence = false;\n      }\n    }).catch(()=> {\n      this.is_maintainence = true;\n    })\n  },\n  mounted() {\n    var doc_height = $(window).height();\n    var body_height = document.querySelector(\"body\").scrollHeight;\n    $(\"footer\").css(\"display\", \"inline-block\");\n    if (\n      this.$route.name != \"Login\" /* && this.$route.name != \"Train the Bot\"*/\n    ) {\n      $(\"footer\").css(\"display\", \"contents\");\n    }\n    bus.$on(\"You can enable bot chat now\", (data) => {\n      if (data == \"True\") {\n        this.show_bot = true;\n      }\n    });\n    bus.$on(\"Disable the bot chat now\", (data1, data2) => {\n      if (data1 == \"True\" && data2 == \"Loggedout\") {\n        this.show_bot = false;\n        bus.$emit(\"destroy bot chat\", \"True\");\n      }\n    });\n    this.show_footer = true;\n    // var widget = GenerateWidget({\n    //   source: \"Web\",\n    //   license_key:\n    //     \"ImNlbnNlYWkyNTkyMkNlbnNlIEFJaHR0cHM6Ly9jZW5zZS5haTIwMjAtMTEtMDkgMTA6MjE6MDkuMzk4MjIzIg.X6kYFQ.YVWptPqqrD2_WVXp1EWab2li_IY\",\n    // });\n    bus.$on(\"is_maintenance_portal\", (data) => {\n      if(data){\n        this.is_maintainence = true;\n      } else {\n        this.is_maintainence = false;\n      }\n    })\n  },\n  methods: {\n    change_margin() {\n      if (this.$route.name == \"Train Model\") {\n        return \"margin-top: 20%;\";\n      } else if (\n        this.$route.name != \"Login\" &&\n        this.$route.name != \"Register\"\n      ) {\n        return \"margin-left:-20%\";\n      }\n    },\n  },\n  watch: {\n    is_maintainence(newVal,oldVal){\n      if(newVal === true){\n        this.$session.set(\"IsPortalMaintainence\", true)\n      } else{\n        this.$session.set(\"IsPortalMaintainence\", false)\n      }\n    }\n  }\n};\nvar doc_height = $(window).height();\nvar body_height = document.querySelector(\"body\").scrollHeight;\n$(\".footer\").css(\"margin-top\", doc_height / 2 + \"px\");\nif (!window.location.href.includes(\"/bot/chat\")) {\n  $(\"footer\").css(\"display\", \"inline-block\");\n}\n</script>\n"],"sourceRoot":"src/portal"}]}