ropdown-menu p-0\" style=\"margin-top: .6rem;\">\n                              <table style=\"width: 100%\">\n                                <tr\n                                  v-for=\"(status, index) in user_list\"\n                                  :key=\"index\"\n                                >\n                                  <td\n                                    style=\"\n                                      display: flex;\n                                      width: auto;\n                                      text-align: center;\n                                    \"\n                                  >\n                                    <input\n                                      v-model=\"selected_user_list\"\n                                      type=\"checkbox\"\n                                      :id=\"\n                                        status.replace(/\\s/g, '') + '_checkbox'\n                                      \"\n                                      :value=\"status\"\n                                      style=\"\n                                        margin-top: auto;\n                                        margin-bottom: auto;\n                                      \"\n                                    />\n                                    <label\n                                      :for=\"\n                                        status.replace(/\\s/g, '') + '_checkbox'\n                                      \"\n                                      :title=\"status\"\n                                      style=\"margin: auto\"\n                                    >\n                                      {{ status }}</label\n                                    >\n                                  </td>\n                                </tr>\n                              </table>\n                            </ul>\n              </th>\n            </tr>\n          </thead>\n          <tbody>\n            