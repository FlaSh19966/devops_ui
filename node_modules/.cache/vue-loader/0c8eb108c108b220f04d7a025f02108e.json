n\"></div>\n    <div class=\"container\">\n      <div class=\"col-sm-12 my-1 p-0\">\n        <div class=\"card-act col-sm-12 p-0\">\n          <button\n            id=\"product_crosssell_save_all_btn\"\n            class=\"darkblue-btn d-block ml-auto\"\n            @click=\"save_product_details\"\n            v-if=\"Boolean(current_product) === true\"\n          >\n            SAVE ALL\n          </button>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-sm-4\">\n          <div class=\"table-box\" v-if=\"Boolean(current_product) === true\">\n            <div class=\"table-responsive\">\n              <table class=\"table\">\n                <thead>\n                  <tr>\n                    <th\n                      scope=\"col\"\n                      class=\"text-left\"\n                      style=\"\n                      font: normal normal normal 16px/22px Roboto;\n                      letter-spacing: 0px;\n                      color: #414141;\n                    \"\n                    >\n                      PRODUCT NAME\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(product, index) in product_data_list\"\n                    :key=\"index\"\n                    style=\"cursor: pointer\"\n                    @click=\"set_current_product(index)\"\n                    :class=\"check_current_product_active(index)\"\n                  >\n                    <td>\n                      {{ product.title }}\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n        <div class=\"col-sm-8\" style=\"overflow :visible;\">\n          <div\n            class=\"table-responsive\"\n            v-if=\"Boolean(current_product) === true\"\n          >\n            <div class=\"px-3 pt-3\">\n              <h6>{{ current_product.title }}</h6>\n            </div>\n            <hr />\n            <div class=\"d-flex px-3 pt-3\">\n              <multiselect\n                v-model=\"current_product.cross_sell_products_list\"\n                :options=\"cross_sell_products\"\n                :multiple=\"true\"\n                :close-on-select=\"false\"\n                :clear-on-select=\"false\"\n                :preserve-search=\"true\"\n                open-direction=\"bottom\"\n                placeholder=\"Pick some products\"\n                label=\"title\"\n                track-by=\"id\"\n                :max=\"limit_cross_sell\"\n              >\n                <template slot=\"selection\" slot-scope=\"{ values, isOpen }\"\n                  ><span\n                    class=\"multiselect__single\"\n                    v-if=\"values.length && !isOpen\"\n                    >{{ values.length }} Products selected</span\n                  ></template\n                >\n                <template slot=\"maxElements\"\n                  ><span class=\"multiselect_single\"\n                    >Maximum of {{ limit_cross_sell }} products selected. Please\n                    remove selected products from the list below.</span\n                  ></template\n                >\n                <template slot=\"tag\">{{ \"\" }}</template>\n              </multiselect>\n            </div>\n            <p v-show=\"current_product.cross_sell_products_list.length > limit_cross_sell\" class=\"p-3\">Maximum of {{ limit_cross_sell }} products are already selected. Please\n              remove selected products from the list below or increase the limit in Settings Tab if you want to add more.</p>\n            <div class=\"mt-5 p-3\">\n              <h5>Cross-Sell Products for {{ current_product.title }}</h5>\n              <div style=\"border-bottom: 1px solid #e3e2e5\"></div>\n              <div\n                v-for=\"(product,\n                index) in current_product.cross_sell_products_list\"\n                :key=\"index\"\n                style=\"border-bottom: 1px solid #e3e2e5\"\n                class=\"py-2 d-flex\"\n              >\n                <span> {{ product.title }} </span>\n                <span\n                  class=\"ml-auto c-p\"\n                  @click=\"remove_button_product_upsell_name(index)\"\n                >\n                  <img\n                    :src=\"require('@/portal/assets/img/delete-from-xd.svg')\"\n                    alt=\"Delete\"\n                  />\n                </span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport api_calls from \"@/portal/api_calls.js\";\nimport Multiselect from \"vue-multiselect\";\nimport Swal from \"sweetalert2\";\nimport { freeze_portal,show_subscription_expired_swal } from \"@/portal/mixins\";\n\nexport default {\n  name: \"ProductsCrosssellPage\",\n  components: {\n    Multiselect\n  },\n  mixins: [freeze_portal,show_subscription_expired_swal],\n  data() {\n    return {\n      spinnerOn: false,\n      company_id: this.$session.get(\"UserInformation\").company_id,\n      company_name: this.$session.get(\"UserInformation\").company_name,\n      product_data_list: [],\n      // new_product_data_list: [],\n      cross_sell_products: [],\n      current_product: null,\n      products_list_data: [],\n      limit_cross_sell: null,\n      retail_web_framework: \"\",\n      disable_btn_expired: false,\n    };\n  },\n  mounted() {\n    this.retail_web_framework = this.$session.get(\"retail_web_framework\");\n    // this.fetch_product_list();\n    setTimeout(() => {\n      this.freeze_portal();\n      }, 1000);\n    this.upsell_crosssell_products_data();\n    this.limit_crossell_products();\n    this.$root.$on(\"ProductsCrosssellPageDisabled\", (data) =>{\n      if(data===true){\n        this.disable_btn_expired = true;\n      }\n    })\n  },\n  methods: {\n    save_product_details() {\n      // var myJSON = JSON.stringify(this.product_data_list);\n      // console.log(\"Cross sell Produccccts :\",myJSON);\n      if(!this.disable_btn_expired){\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: `You want to save these products data?`,\n        type: \"question\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, Confirm!\"\n      }).then(result => {\n        if (result.value) {\n          this.spinnerOn = true;\n          axios\n            .post(\n              api_calls.upsell_crosssell(),\n              {\n                company_id: this.company_id,\n                company_name: this.company_name,\n                is_save_edit: true,\n                products_data: this.product_data_list,\n                is_crosssell_data: true\n              },\n              {\n                headers: {\n                  Authorization: `Bearer ${this.$session.get(\"at\")}`\n                }\n              }\n            )\n            .then(response => {\n              // console.log(response);\n              this.spinnerOn = false;\n              if (response.data == \"Query Executed Successfully\") {\n                Swal({\n                  type: \"success\",\n                  text: \"Products data added successfully\",\n                  timer: 2500\n                }).then(result => {\n                  this.upsell_crosssell_products_data();\n                });\n              }\n            })\n            .catch(e => {\n              this.spinnerOn = false;\n              if (\n                e.response.status === 410 ||\n                e.response.status === 440 ||\n                e.response.status === 409\n              ) {\n                this.$root.$emit(\"Session_Expired\", e.response.data);\n              }\n            });\n        }\n      });}else{\n        this.show_subscription_expired_swal();\n      }\n    },\n    // fetch_product_list() {\n    //   this.spinnerOn = true;\n    //   axios\n    //     .post(\n    //       api_calls.template_synonyms(),\n    //       {\n    //         company_id: this.company_id,\n    //         company_name: this.company_name,\n    //         is_get_product_list: true\n    //       },\n    //       {\n    //         headers: {\n    //           Authorization: `Bearer ${this.$session.get(\"at\")}`\n    //         }\n    //       }\n    //     )\n    //     .then(({ data }) => {\n    //       if (data.status === \"Success\") {\n    //         var temp;\n    //         temp = data.products_data;\n    //         if (this.retail_web_framework == \"woocommerce\") {\n    //           for (var i in temp) {\n    //             //create a new array object new_products_is_any\n    //             this.new_product_data_list.push({\n    //               id: temp[i].id,\n    //               title: temp[i].name,\n    //               cross_sell_products_list: []\n    //             });\n    //             this.cross_sell_products.push({\n    //               id: temp[i].id,\n    //               title: temp[i].name\n    //             });\n    //           }\n    //         }else if(this.retail_web_framework == \"shopify\"){\n    //           for (var i in temp) {\n    //             //create a new array object new_products_is_any\n    //             this.new_product_data_list.push({\n    //               id: temp[i].id,\n    //               title: temp[i].title,\n    //               cross_sell_products_list: []\n    //             });\n    //             this.cross_sell_products.push({\n    //               id: temp[i].id,\n    //               title: temp[i].title\n    //             });\n    //           }\n    //         }\n    //         // console.log(\"new product data\", this.new_product_data_list);\n    //         // this.spinnerOn = false;\n    //         this.upsell_crosssell_products_data();\n    //         // if (this.product_data_list.length > 0) {\n    //         //   this.current_product = this.product_data_list[0];\n    //         // }\n    //       }\n    //     })\n    //     .catch(e => {\n    //       this.spinnerOn = false;\n    //       if (\n    //         e.response.status === 410 ||\n    //         e.response.status === 440 ||\n    //         e.response.status === 409\n    //       ) {\n    //         this.$root.$emit(\"Session_Expired\", e.response.data);\n    //       }\n    //     });\n    // },\n    upsell_crosssell_products_data() {\n      this.product_data_list = [];\n      this.spinnerOn = true;\n      axios\n        .post(\n          api_calls.upsell_crosssell(),\n          {\n            company_id: this.company_id,\n            company_name: this.company_name,\n            is_get: true,\n            is_crosssell_data: true\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${this.$session.get(\"at\")}`\n            }\n          }\n        )\n        .then(response => {\n          if (response.data != null) {\n            // console.log(response.data.cross_sell);\n            var temp = response.data.cross_sell;\n            for (var i in temp) {\n              this.product_data_list.push({\n                id: temp[i].id,\n                title: temp[i].title,\n                cross_sell_products_list: temp[i].cross_sell_products_list ? temp[i].cross_sell_products_list : [] \n              });\n              this.cross_sell_products.push({\n                  id: temp[i].id,\n                  title: temp[i].title\n                });\n            }\n            // console.log(\"before old product data\",this.product_data_list);\n            // var temp2 = this.new_product_data_list;\n            // var temp3 = this.product_data_list;\n\n            // var res = temp2.filter(\n            //   item1 =>\n            //     !temp3.some(\n            //       item2 => item2.id === item1.id && item2.name === item1.name\n            //     )\n            // );\n            // debugger;\n            // // console.log(res);\n            // for (var i in res) {\n            //   this.product_data_list.push({\n            //     id: res[i].id,\n            //     title: res[i].title,\n            //     cross_sell_products_list: []\n            //   });\n            // }\n            // console.log(\"after old product data\",this.product_data_list);\n            if (this.product_data_list.length > 0) {\n              this.current_product = this.product_data_list[0];\n            }\n            setTimeout(() => {\n              this.freeze_portal();\n            }, 1000);\n          }\n          this.spinnerOn = false;\n        })\n        .catch(e => {\n          this.spinnerOn = false;\n          if (\n            e.response.status === 410 ||\n            e.response.status === 440 ||\n            e.response.status === 409\n          ) {\n            this.$root.$emit(\"Session_Expired\", e.response.data);\n          }\n        });\n    },\n    limit_crossell_products() {\n      this.limit_cross_sell = 10;\n      axios\n        .post(\n          api_calls.product_settings(),\n          {\n            company_name: this.company_name,\n            company_id: this.company_id,\n            email: this.$session.get(\"UserInformation\").email,\n            license_key: this.$session.get(\"UserInformation\").license_key,\n            token: this.$session.get(\"UserInformation\").tokens,\n            is_get: true\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${this.$session.get(\"at\")}`\n            }\n          }\n        )\n        .then(response => {\n          if (\n            response.data != \"\" &&\n            response.data != null &&\n            response.data != \"Internal server Error\"\n          ) {\n            if(this.retail_web_framework == \"woocommerce\"){\n              this.limit_cross_sell = response.data.woocommerce_details.limit_up_sell;\n            }\n            else if(this.retail_web_framework == \"shopify\"){\n              this.limit_cross_sell = response.data.shopify_details.limit_up_sell;\n            }\n          } else {\n            toastr.error(\"Internal Server Error\");\n          }\n        })\n        .catch(e => {\n          this.spinnerOn = false;\n          toastr.error(\"Some Error Occurred.\");\n          if (\n            e.response.status === 410 ||\n            e.response.status === 440 ||\n            e.response.status === 409\n          ) {\n            this.$root.$emit(\"Session_Expired\", e.response.data);\n          }\n        });\n    },\n    check_current_product_active(index) {\n      if (Boolean(this.current_product) === true) {\n        return this.current_product.title ===\n          this.product_data_list[index].title\n          ? \"current-product\"\n          : null;\n      }\n    },\n    remove_button_product_upsell_name(index) {\n      this.current_product.cross_sell_products_list.splice(index, 1);\n    },\n    set_current_product(index) {\n      this.current_product = this.product_data_list[index];\n    }\n  }\n};\n</script>\n\n<style scoped>\n.alternate_products {\n  font-family: \"Roboto\";\n}\n.table-box tbody tr:hover td,\n.table-box tbody tr:hover th {\n  background: #ececec;\n}\n.table th {\n  border-top: none;\n}\n.table-responsive {\n  height: 100vh;\n  overflow-y: auto;\n  border: 1px solid #e3e2e5;\n}\n.table-responsive thead th {\n  top: 0;\n  position: sticky;\n}\n.table-responsive::-webkit-scrollbar {\n  max-height: 10px;\n  width: 7px;\n  height: 7px;\n  border-radius: 10px;\n  overflow-x: hidden;\n}\n.table-responsive::-webkit-scrollbar-track {\n  background: #ffffff;\n  border-radius: 10px;\n}\n.table-responsive::-webkit-scrollbar-thumb {\n  background: #27397b;\n  border-radius: 10px;\n}\n.table-responsive::-webkit-scrollbar-thumb:hover {\n  background: #19295f;\n  border-radius: 10px;\n}\n.form-control {\n  line-height: 16px;\n  padding: 10px 15px;\n}\n.current-product {\n  background: rgba(0, 0, 0, 0.1);\n}\n</style>\n"]}]}